lass=&quot;line&quot;&gt;myDate.getTime(); &lt;span class=&quot;comment&quot;&gt;//获取当前时间(从1970.1.1开始的毫秒数)&lt;/span&gt;&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;myDate.getHours(); &lt;span class=&quot;comment&quot;&gt;//获取当前小时数(0-23)&lt;/span&gt;&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;myDate.getMinutes(); &lt;span class=&quot;comment&quot;&gt;//获取当前分钟数(0-59)&lt;/span&gt;&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;myDate.getSeconds(); &lt;span class=&quot;comment&quot;&gt;//获取当前秒数(0-59)&lt;/span&gt;&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;myDate.getMilliseconds(); &lt;span class=&quot;comment&quot;&gt;//获取当前毫秒数(0-999)&lt;/span&gt;&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;myDate.toLocaleDateString(); &lt;span class=&quot;comment&quot;&gt;//获取当前日期&lt;/span&gt;&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;&lt;span class=&quot;keyword&quot;&gt;var&lt;/span&gt; mytime=myDate.toLocaleTimeString(); &lt;span class=&quot;comment&quot;&gt;//获取当前时间&lt;/span&gt;&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;myDate.toLocaleString( ); &lt;span class=&quot;comment&quot;&gt;//获取日期与时间&lt;/span&gt;&lt;/span&gt;&lt;br&gt;&lt;/pre&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/table&gt;&lt;/figure&gt;
    
